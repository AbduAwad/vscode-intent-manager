{
  "name": "nokia-intent-manager",
  "displayName": "NOKIA_IM",
  "description": "NOKIA IM vsCode Developer Plugin",
  "version": "2.1.1",
  "icon": "media/NSP_Logo.png",
  "publisher": "Nokia",
  "repository": "http://github.com/nokia/vscode-intent-manager",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onFileSystem:im",
    "onStartupFinished"
  ],
  "main": "./dist/main.js",
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets.json"
      }
    ],
    "commands": [
      {
        "command": "nokia-intent-manager.audit",
        "category": "IntentManager",
        "title": "Audit intent(s)",
        "icon": "$(search)"
      },
      {
        "command": "nokia-intent-manager.lastAuditReport",
        "category": "IntentManager",
        "title": "Audit report",
        "icon": "$(search)"
      },
      {
        "command": "nokia-intent-manager.sync",
        "category": "IntentManager",
        "title": "Synchronize intent(s)",
        "icon": "$(sync)"
      },
      {
        "command": "nokia-intent-manager.logs",
        "category": "IntentManager",
        "title": "Server Logs",
        "icon": "$(debug)"
      },
      {
        "command": "nokia-intent-manager.state",
        "category": "IntentManager",
        "title": "Set intent(s) state",
        "icon": "$(symbol-property)"
      },
      {
        "command": "nokia-intent-manager.openInBrowser",
        "category": "IntentManager",
        "title": "Open in browser",
        "icon": "$(home)"
      },
      {
        "command": "nokia-intent-manager.uploadIntentType",
        "category": "IntentManager",
        "title": "Upload intent-type",
        "icon": "$(cloud-upload)"
      },
      {
        "command": "nokia-intent-manager.uploadIntents",
        "category": "IntentManager",
        "title": "Upload intent(s)",
        "icon": "$(cloud-upload)"
      },
      {
        "command": "workbench.files.action.compareWithSaved",
        "category": "IntentManager",
        "title": "Compare changes",
        "icon": "$(request-changes)"
      },
      {
        "command": "nokia-intent-manager.newVersion",
        "category": "IntentManager",
        "title": "New intent-type version"
      },
      {
        "command": "nokia-intent-manager.clone",
        "category": "IntentManager",
        "title": "Clone intent-type"
      },
      {
        "command": "nokia-intent-manager.newIntent",
        "category": "IntentManager",
        "title": "Create intent",
        "icon": "$(play-circle)"
      },
      {
        "command": "nokia-intent-manager.newIntentType",
        "category": "IntentManager",
        "title": "Create intent-type"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "when": "resourceScheme != im && resourceFilename == meta-info.json || resourceFilename == script-content.js || resourceFilename == script-content.mjs",
          "command": "nokia-intent-manager.uploadIntentType",
          "group": "navigation"
        },
        {
          "when": "resourceScheme == im && activeEditorIsDirty == true",
          "command": "workbench.files.action.compareWithSaved",
          "group": "navigation"
        },
        {
          "when": "resourceScheme == im",
          "command": "nokia-intent-manager.openInBrowser",
          "group": "navigation"
        },
        {
          "when": "resource =~ /^im:\\/[a-z][a-z0-9_\\-]*_v\\d+\\//",
          "command": "nokia-intent-manager.newIntent",
          "group": "navigation"
        },
        {
          "when": "resource =~ /^im:\\/[a-z][a-z0-9_\\-]*_v\\d+\\/intents\\/[^/]+$/",
          "command": "nokia-intent-manager.logs",
          "group": "navigation"
        },
        {
          "when": "resource =~ /^im:\\/[a-z][a-z0-9_\\-]*_v\\d+\\/intents\\/[^/]+$/",
          "command": "nokia-intent-manager.audit",
          "group": "navigation"
        },
        {
          "when": "resource =~ /^im:\\/[a-z][a-z0-9_\\-]*_v\\d+\\/intents\\/[^/]+$/",
          "command": "nokia-intent-manager.sync",
          "group": "navigation"
        },
        {
          "when": "resource =~ /^im:\\/[a-z][a-z0-9_\\-]*_v\\d+\\/intents\\/[^/]+$/",
          "command": "nokia-intent-manager.state",
          "group": "navigation"
        }
      ],
      "explorer/context": [
        {
          "command": "nokia-intent-manager.uploadIntentType",
          "group": "nokia_im",
          "when": "resource =~ /^file:.*\\/intent\\-[a-z][a-z0-9_\\-]*\\-v\\d+/ || resource =~ /^file:.*\\/[a-z][a-z0-9_\\-]*_v\\d+/"
        },
        {
          "command": "nokia-intent-manager.uploadIntents",
          "group": "nokia_im",
          "when": "resource =~ /^file:.*\\/[a-z][a-z0-9_\\-]*_v\\d+\\/intents\\/.+\\.json/"
        },
        {
          "command": "nokia-intent-manager.newIntentType",
          "group": "nokia_im",
          "when": "resource =~ /^im:\\/$/"
        },
        {
          "command": "nokia-intent-manager.newIntent",
          "group": "nokia_im",
          "when": "resource =~ /^im:\\/[a-z][a-z0-9_\\-]*_v\\d+/"
        },
        {
          "command": "nokia-intent-manager.newVersion",
          "group": "nokia_im",
          "when": "resource =~ /^im:\\/[a-z][a-z0-9_\\-]*_v\\d+/"
        },
        {
          "command": "nokia-intent-manager.clone",
          "group": "nokia_im",
          "when": "resource =~ /^im:\\/[a-z][a-z0-9_\\-]*_v\\d+/"
        },
        {
          "command": "nokia-intent-manager.logs",
          "group": "nokia_im",
          "when": "resource =~ /^im:\\/[a-z][a-z0-9_\\-]*_v\\d+/"
        },
        {
          "command": "nokia-intent-manager.audit",
          "group": "nokia_im",
          "when": "resource =~ /^im:\\/[a-z][a-z0-9_\\-]*_v\\d+\\/intents\\/[^/]+$/"
        },
        {
          "command": "nokia-intent-manager.sync",
          "group": "nokia_im",
          "when": "resource =~ /^im:\\/[a-z][a-z0-9_\\-]*_v\\d+\\/intents\\/[^/]+$/"
        },
        {
          "command": "nokia-intent-manager.state",
          "group": "nokia_im",
          "when": "resource =~ /^im:\\/[a-z][a-z0-9_\\-]*_v\\d+\\/intents\\/[^/]+$/"
        },
        {
          "command": "nokia-intent-manager.lastAuditReport",
          "group": "nokia_im",
          "when": "resource =~ /^im:\\/[a-z][a-z0-9_\\-]*_v\\d+\\/intents\\/[^/]+$/"
        },
        {
          "command": "nokia-intent-manager.openInBrowser",
          "group": "nokia_im",
          "when": "resourceScheme == im"
        }
      ]
    },
    "configuration": {
      "title": "IntentManager",
      "properties": {
        "intentManager.NSPIP": {
          "type": "string",
          "default": "localhost",
          "scope": "application",
          "format": "ipv4",
          "description": "NSP IP address."
        },
        "intentManager.port": {
          "type": "string",
          "default": "8545",
          "scope": "application",
          "enum": [
            "443",
            "8545"
          ],
          "enumDescriptions": [
            "443 port enabled in newer releases of NSP. 8545 to be deprecated.",
            "8545 used in older NSP releases, to be deprecated."
          ],
          "description": "NSP IM port."
        },
        "intentManager.user": {
          "type": "string",
          "default": "user",
          "scope": "application",
          "description": "NSP user."
        },
        "intentManager.password": {
          "type": "null",
          "markdownDescription": "[Set Password](command:nokia-intent-manager.setPassword)",
          "scope": "application",
          "description": "NSP user password."
        },
        "intentManager.timeout": {
          "type": "number",
          "default": 90000,
          "scope": "application",
          "description": "timeout for calls to NSP in ms"
        },
        "intentManager.ignoreLabels": {
          "type": "array",
          "scope": "application",
          "items": {
            "type": "string"
          },
          "description": "Intent-types with these labels will be hidden."
        },
        "intentManager.parallelOperations.enable": {
          "type": "boolean",
          "scope": "application",
          "default": false,
          "description": "Enable parallel intent operations (EXPERIMENTAL)"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run esbuild -- --minify",
    "compile": "tsc -p ./ --sourceMap",
    "lint": "eslint \"src/**/*.ts\"",
    "clean": "rm -rf node_modules out",
    "watch": "tsc -watch -p ./",
    "build": "tsc",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=dist/main.js --external:vscode --format=cjs --platform=node --packages=external",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "esbuild": "npm run esbuild-base -- --sourcemap"
  },
  "devDependencies": {
    "@types/node": "18.x",
    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "esbuild": "^0.21.5",
    "eslint": "^8.26.0",
    "typescript": "5.x"
  },
  "dependencies": {
    "@vscode/codicons": "^0.0.36",
    "base-64": "^1.0.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.7.0",
    "npm": "^10.8.1",
    "nunjucks": "^3.2.0",
    "vscode-uri": "^3.0.8",
    "vse": "^0.5.1",
    "yaml": "^2.4.5"
  }
}
