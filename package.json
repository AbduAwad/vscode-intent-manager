{
  "name": "nokia-intent-manager",
  "displayName": "NOKIA_IM",
  "description": "NOKIA IM vsCode Developer Plugin",
  "version": "2.0.0",
  "icon": "media/NSP_Logo.png",
  "publisher": "Nokia",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onFileSystem:im",
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets.json"
      }
    ],
    "commands": [
      {
        "command": "nokia-intent-manager.audit",
        "category": "IntentManager",
        "title": "Audit intent(s)",
        "icon": "$(search)"
      },
      {
        "command": "nokia-intent-manager.lastAuditReport",
        "category": "IntentManager",
        "title": "Last audit report",
        "icon": "$(search)"
      },
      {
        "command": "nokia-intent-manager.sync",
        "category": "IntentManager",
        "title": "Synchronize intent(s)",
        "icon": "$(sync)"
      },
      {
        "command": "nokia-intent-manager.logs",
        "category": "IntentManager",
        "title": "NSP server-logs",
        "icon": "$(debug)"
      },
      {
        "command": "nokia-intent-manager.state",
        "category": "IntentManager",
        "title": "Set intent state",
        "icon": "$(symbol-property)"
      },
      {
        "command": "nokia-intent-manager.openInBrowser",
        "category": "IntentManager",
        "title": "Open in browser",
        "icon": "$(home)"
      },
      {
        "command": "nokia-intent-manager.uploadLocal",
        "category": "IntentManager",
        "title": "Upload intent-type",
        "icon": "$(cloud-upload)"
      },
      {
        "command": "workbench.files.action.compareWithSaved",
        "category": "IntentManager",
        "title": "Compare changes",
        "icon": "$(request-changes)"
      },
      {
        "command": "nokia-intent-manager.newVersion",
        "category": "IntentManager",
        "title": "New intent-type version"
      },
      {
        "command": "nokia-intent-manager.clone",
        "category": "IntentManager",
        "title": "Clone intent-type"
      },
      {
        "command": "nokia-intent-manager.newIntent",
        "category": "IntentManager",
        "title": "Create new intent",
        "icon": "$(play-circle)"
      },
      {
        "command": "nokia-intent-manager.newIntentType",
        "category": "IntentManager",
        "title": "Create new intent-type"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "when": "resourceScheme != im && resourceFilename == meta-info.json || resourceFilename == script-content.js || resourceFilename == script-content.mjs",
          "command": "nokia-intent-manager.uploadLocal",
          "group": "nokia_im"
        },
        {
          "when": "resourceScheme == im && activeEditorIsDirty == true",
          "command": "workbench.files.action.compareWithSaved",
          "group": "nokia_im"
        },
        {
          "when": "resourceScheme == im",
          "command": "nokia-intent-manager.openInBrowser",
          "group": "nokia_im"
        },
        {
          "when": "resource =~ /^im:\\/[a-zA-Z0-9\\-_]+_v\\d+\\//",
          "command": "nokia-intent-manager.newIntent",
          "group": "nokia_im"
        },
        {
          "when": "resource =~ /^im:\\/[a-zA-Z0-9\\-_]+_v\\d+\\/intents\\/[^/]+$/",
          "command": "nokia-intent-manager.logs",
          "group": "nokia_im"
        },
        {
          "when": "resource =~ /^im:\\/[a-zA-Z0-9\\-_]+_v\\d+\\/intents\\/[^/]+$/",
          "command": "nokia-intent-manager.audit",
          "group": "nokia_im"
        },
        {
          "when": "resource =~ /^im:\\/[a-zA-Z0-9\\-_]+_v\\d+\\/intents\\/[^/]+$/",
          "command": "nokia-intent-manager.sync",
          "group": "nokia_im"
        },
        {
          "when": "resource =~ /^im:\\/[a-zA-Z0-9\\-_]+_v\\d+\\/intents\\/[^/]+$/",
          "command": "nokia-intent-manager.state",
          "group": "nokia_im"
        }
      ],
      "explorer/context": [
        {
          "command": "nokia-intent-manager.uploadLocal",
          "group": "nokia_im",
          "when": "resource =~ /^file:.*\\/intent\\-[a-z][a-zA-Z0-9\\-_]*\\-v\\d+/ || resource =~ /^file:.*\\/[a-z][a-zA-Z0-9\\-_]*_v\\d+/"
        },
        {
          "command": "nokia-intent-manager.newIntentType",
          "group": "nokia_im",
          "when": "resourceScheme == file && resourceExtname == .yang"
        },
        {
          "command": "nokia-intent-manager.newIntentType",
          "group": "nokia_im",
          "when": "resource =~ /^im:\\/$/"
        },
        {
          "command": "nokia-intent-manager.newIntent",
          "group": "nokia_im",
          "when": "resource =~ /^im:\\/[a-z][a-zA-Z0-9\\-_]*_v\\d+/"
        },
        {
          "command": "nokia-intent-manager.newVersion",
          "group": "nokia_im",
          "when": "resource =~ /^im:\\/[a-z][a-zA-Z0-9\\-_]*_v\\d+/"
        },
        {
          "command": "nokia-intent-manager.clone",
          "group": "nokia_im",
          "when": "resource =~ /^im:\\/[a-z][a-zA-Z0-9\\-_]*_v\\d+/"
        },
        {
          "command": "nokia-intent-manager.logs",
          "group": "nokia_im",
          "when": "resource =~ /^im:\\/[a-z][a-zA-Z0-9\\-_]*_v\\d+/"
        },
        {
          "command": "nokia-intent-manager.audit",
          "group": "nokia_im",
          "when": "resource =~ /^im:\\/[a-z][a-zA-Z0-9\\-_]*_v\\d+\\/intents\\/[^/]+$/"
        },
        {
          "command": "nokia-intent-manager.sync",
          "group": "nokia_im",
          "when": "resource =~ /^im:\\/[a-z][a-zA-Z0-9\\-_]*_v\\d+\\/intents\\/[^/]+$/"
        },
        {
          "command": "nokia-intent-manager.state",
          "group": "nokia_im",
          "when": "resource =~ /^im:\\/[a-z][a-zA-Z0-9\\-_]*_v\\d+\\/intents\\/[^/]+$/"
        },
        {
          "command": "nokia-intent-manager.lastAuditReport",
          "group": "nokia_im",
          "when": "resource =~ /^im:\\/[a-z][a-zA-Z0-9\\-_]*_v\\d+\\/intents\\/[^/]+$/"
        }
      ]
    },
    "configuration": {
      "title": "IntentManager",
      "properties": {
        "intentManager.NSPIP": {
          "type": "string",
          "default": "localhost",
          "scope": "application",
          "format": "ipv4",
          "description": "NSP IP address."
        },
        "intentManager.port": {
          "type": "string",
          "default": "8545",
          "scope": "application",
          "enum": [
            "443",
            "8545"
          ],
          "enumDescriptions": [
            "443 port enabled in newer releases of NSP. 8545 to be deprecated.",
            "8545 used in older NSP releases, to be deprecated."
          ],
          "description": "NSP IM port."
        },
        "intentManager.user": {
          "type": "string",
          "default": "user",
          "scope": "application",
          "description": "NSP user."
        },
        "intentManager.password": {
          "type": "null",
          "markdownDescription": "[Set Password](command:nokia-intent-manager.setPassword)",
          "scope": "application",
          "description": "NSP user password."
        },
        "intentManager.timeout": {
          "type": "number",
          "default": 20000,
          "scope": "application",
          "description": "Connection timeout in ms"
        },
        "intentManager.ignoreLabels": {
          "type": "array",
          "scope": "application",
          "items": {
            "type": "string"
          },
          "description": "Intent-types with these labels will be hidden."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "build": "./node_modules/typescript/bin/tsc",
    "compile": "./node_modules/typescript/bin/tsc -p ./",
    "watch": "./node_modules/typescript/bin/tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node --packages=external",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "test-compile": "./node_modules/typescript/bin/tsc -p ./"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@vscode/codicons": "0.0.20",
    "@vscode/vsce": "^2.25.0",
    "base-64": "^1.0.0",
    "esbuild": "^0.20.2",
    "lodash": "^4.17.0",
    "node-fetch": "2.6.6",
    "nunjucks": "^3.2.0",
    "vsce": "^2.15.0",
    "vscode-uri": "^3.0.0",
    "vse": "^0.5.1",
    "yaml": "^2.2.0"
  }
}
